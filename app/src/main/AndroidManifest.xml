<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="weatherapp.ec463">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="WeatherApp"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="weatherapp.ec463.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>

        <activity
            android:name="weatherapp.ec463.DataSelectActvity"></activity>

        <activity
            android:name="weatherapp.ec463.Temperature"></activity>

        <activity
            android:name="weatherapp.ec463.Humidity"></activity>

        <activity
            android:name="weatherapp.ec463.LocationSelectionActivity"></activity>

    </application>

</manifest>